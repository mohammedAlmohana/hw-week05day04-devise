<p id="notice"><%= notice %></p>

<h1>Cars1s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Model</th>
      <th>Year</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars1s.each do |cars1| %>
      <tr>
        <td><%= cars1.name %></td>
        <td><%= cars1.model %></td>
        <td><%= cars1.year %></td>
        <td><%= cars1.user_id %></td>
        <td><%= link_to 'Show', cars1 %></td>
        <td><%= link_to 'Edit', edit_cars1_path(cars1) %></td>
        <td><%= link_to 'Destroy', cars1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cars1', new_cars1_path %>
